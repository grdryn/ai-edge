{
    "apiVersion": "apps/v1",
    "kind": "Deployment",
    "metadata": {
        "name": "tensorflow-housing-app-model-1",
        "namespace": "tensorflow-housing-app"
    },
    "spec": {
        "progressDeadlineSeconds": 600,
        "replicas": 1,
        "revisionHistoryLimit": 10,
        "selector": {
            "matchLabels": {
                "app": "tensorflow-housing-app-1"
            }
        },
        "strategy": {
            "rollingUpdate": {
                "maxSurge": "25%",
                "maxUnavailable": "25%"
            },
            "type": "RollingUpdate"
        },
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "app": "tensorflow-housing-app-1"
                }
            },
            "spec": {
                "containers": [
                    {
                        "image": "quay.io/grdryn/tensorflow-housing@sha256:c9dc2c47999738d95625bfb9bef3ec81c9f8a280db45888d3926ddbbc645a4c7",
                        "imagePullPolicy": "IfNotPresent",
                        "livenessProbe": {
                            "failureThreshold": 8,
                            "httpGet": {
                                "path": "/v2/health/live",
                                "port": 8080,
                                "scheme": "HTTP"
                            },
                            "periodSeconds": 5,
                            "successThreshold": 1,
                            "timeoutSeconds": 1
                        },
                        "name": "model",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ],
                        "readinessProbe": {
                            "failureThreshold": 8,
                            "httpGet": {
                                "path": "/v2/models/tensorflow-housing/ready",
                                "port": 8080,
                                "scheme": "HTTP"
                            },
                            "periodSeconds": 5,
                            "successThreshold": 1,
                            "timeoutSeconds": 1
                        },
                        "resources": {},
                        "terminationMessagePath": "/dev/termination-log",
                        "terminationMessagePolicy": "File"
                    }
                ],
                "dnsPolicy": "ClusterFirst",
                "restartPolicy": "Always",
                "schedulerName": "default-scheduler",
                "securityContext": {},
                "terminationGracePeriodSeconds": 30
            }
        }
    }
}
